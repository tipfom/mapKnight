<Window x:Class="mapKnight.ToolKit.Windows.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mapKnight.ToolKit"
        mc:Ignorable="d"
        Title="About" WindowStartupLocation="CenterOwner" SizeToContent="WidthAndHeight" Icon="{StaticResource ResourceKey=logo}" ResizeMode="NoResize" Loaded="Window_Loaded">
    <StackPanel>
        <TextBlock Margin="5,5,5,5" Text="Pluto Alpha made by Central Potato Unit Studios"/>
        <TextBlock Margin="5,0,5,0" x:Name="label_version" Text="Version"/>
        <TextBlock Margin="5,5,5,5" x:Name="label_mkversion" Text="Designed for mapKnight Version"/>
    </StackPanel>
</Window>
